@using EntityLayer.Concrete
@model List<About>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<div class="container">
    <h2 class="text-center">Hakkımda Veri Listesi</h2>
    <table class="table table-hover">
        <tr>
            <th>Id</th>
            <th>Hakkımda Başlığı</th>
            <th>Aktif Yap</th>
            <th>Pasif Yap</th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.AboutId</td>
                <td>Başlık</td>
                <td><a class="btn btn-success">Aktif Yap</a></td>
                <td><a class="btn btn-danger">Pasif Yap</a></td>
            </tr>
        }
    </table>
    @Html.Action("AboutPartial", "About")
</div>
